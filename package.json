{
	"name": "app",
	"version": "1.0.0",
	"description": "Monetizzer API",
	"author": "Monetizzer",
	"private": false,
	"license": "Apache-2.0",
	"dependencies": {
		"@aws-sdk/client-cloudformation": "^3.181.0",
		"@aws-sdk/client-dynamodb": "^3.179.0",
		"@aws-sdk/client-s3": "^3.179.0",
		"@aws-sdk/client-sns": "^3.179.0",
		"@aws-sdk/client-sqs": "^3.179.0",
		"@aws-sdk/client-ssm": "^3.180.0",
		"@aws-sdk/lib-storage": "^3.179.0",
		"@aws-sdk/s3-presigned-post": "^3.186.0",
		"@aws-sdk/util-dynamodb": "^3.179.0",
		"@discordjs/rest": "^1.2.0",
		"@techmmunity/utils": "^1.10.1",
		"axios": "^0.27.2",
		"change-case": "^4.1.2",
		"fastify": "^4.6.0",
		"uid": "^2.0.0",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@serverless/typescript": "^3.21.0",
		"@techmmunity/eslint-config": "^5.2.4",
		"@types/aws-lambda": "^8.10.106",
		"@types/jest": "^29.0.3",
		"@types/node": "^18.7.23",
		"@types/supertest": "^2.0.12",
		"@types/uuid": "^8.3.3",
		"@vercel/ncc": "^0.34.0",
		"aws-sdk": "^2.1224.0",
		"discord-api-types": "^0.37.11",
		"env-cmd": "^10.1.0",
		"eslint": "^8.24.0",
		"husky": "^8.0.1",
		"jest": "^29.0.3",
		"lint-staged": "^13.0.3",
		"lodash": "^4.17.21",
		"prettier": "^2.7.1",
		"serverless": "^3.22.0",
		"serverless-localstack": "^1.0.0",
		"serverless-webpack": "^5.10.0",
		"supertest": "^6.2.4",
		"terser-webpack-plugin": "^5.3.6",
		"ts-jest": "^29.0.2",
		"ts-loader": "^9.4.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.3",
		"webpack": "^5.74.0"
	},
	"scripts": {
		"ts-node": "ts-node",
		"prepare": "husky install",
		"build": "serverless package",
		"build:api": "ncc build src/server.ts -o api -m -s -e aws-sdk -q",
		"start:api": "node index.js",
		"deploy:production": "NODE_ENV=production env-cmd serverless deploy --stage production",
		"deploy:dev": "NODE_ENV=dev env-cmd serverless deploy --stage dev",
		"deploy:local": "NODE_ENV=local env-cmd serverless deploy --stage local",
		"deploy:production:all": "./scripts/deploy-serverless-production.sh",
		"deploy:dev:all": "./scripts/deploy-serverless-dev.sh",
		"deploy:local:all": "./scripts/deploy-serverless-local.sh",
		"docker:server": "docker-compose up --build docker-events-listener localstack",
		"docker:lambda": "docker-compose up --build lambda",
		"docker:api": "docker-compose up --build api",
		"format": "eslint . --fix --quiet",
		"lint": "eslint . --quiet",
		"test": "jest --passWithNoTests",
		"test:cov": "jest --coverage --passWithNoTests",
		"upgrade-dependencies": "yarn upgrade-interactive --latest",
		"husky:pre-commit": "tsc && yarn lint"
	}
}
