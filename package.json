		"uid": "^2.0.0",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@serverless/typescript": "^3.21.0",
		"@techmmunity/eslint-config": "^5.2.4",
		"@types/aws-lambda": "^8.10.106",
		"@types/jest": "^29.0.3",
		"@types/jsonwebtoken": "^8.5.9",
		"@types/node": "^18.7.23",
		"@types/supertest": "^2.0.12",
		"@types/uuid": "^8.3.3",
		"aws-sdk": "^2.1224.0",
		"change-case": "^4.1.2",
		"dotenv": "^16.0.2",
		"eslint": "^8.24.0",
		"husky": "^8.0.1",
		"jest": "^29.0.3",
		"lint-staged": "^13.0.3",
		"lodash": "^4.17.21",
		"prettier": "^2.7.1",
		"serverless": "^3.22.0",
		"serverless-localstack": "^1.0.0",
		"serverless-webpack": "^5.10.0",
		"supertest": "^6.2.4",
		"terser-webpack-plugin": "^5.3.6",
		"ts-jest": "^29.0.2",
		"ts-loader": "^9.4.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.3",
		"webpack": "^5.74.0",
		"webpack-node-externals": "^3.0.0"
	},
	"scripts": {
		"ts-node": "ts-node -r tsconfig-paths/register",
		"prepare": "husky install",
		"build": "serverless package",
		"start": "node dist/index.js",
		"serverless:deploy": "serverless deploy",
		"deploy:production": "NODE_ENV=production serverless deploy --stage production",
		"deploy:dev": "NODE_ENV=dev serverless deploy --stage dev",
		"deploy:local": "NODE_ENV=local serverless deploy --stage local",
		"docker:server": "docker-compose up --build docker-events-listener localstack",
		"docker:func": "docker-compose up --build api",
		"format": "eslint . --fix --quiet",
		"lint": "eslint . --quiet",
		"test": "jest --passWithNoTests",
		"test:cov": "jest --coverage --passWithNoTests",
		"upgrade-dependencies": "yarn upgrade-interactive --latest",
		"temp:test-tsc": "tsc",
		"husky:pre-commit": "tsc && yarn lint"
	}
}
